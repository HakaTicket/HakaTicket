<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:windowSoftInputMode="stateHidden"
    package="bks.colibriCorporation.hakaticket">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.Camera" />
    <uses-feature android:name="android.hardware.Camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="bks.colibriCorporation.hakaticket.CGUActivity"></activity>
        <activity android:name="bks.colibriCorporation.hakaticket.BottomNavigationActivity" />
        <activity android:name="bks.colibriCorporation.hakaticket.LoginActivity" />
        <activity android:name="bks.colibriCorporation.hakaticket.RegisterActivity" />
        <activity android:name="bks.colibriCorporation.hakaticket.ParameterActivity"/>
        <activity android:name="bks.colibriCorporation.hakaticket.HistoryActivity"/>
        <activity android:name="bks.colibriCorporation.hakaticket.ScanActivity">
        <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>